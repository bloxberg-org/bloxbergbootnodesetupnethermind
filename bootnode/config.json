{
  "Init": {
    "BaseDbPath": "/nethermind/nethermind_db/bloxberg",
    "ChainSpecPath": "/nethermind/chainspec/bloxberg.json",
    "StaticNodesPath": "/nethermind/Data/static-nodes-bloxberg.json",
    "WebSocketsEnabled": true,
    "DiscoveryEnabled": true
  },
  "Network": {
    "MaxActivePeers": 256,
    "DiscoveryPort": 30303,
    "P2PPort": 30303,
    "DiagTracerEnabled": false
  },
  "JsonRpc": {
    "Enabled": true,
    "EnabledModules": [
      "Eth",
      "Subscribe",
      "Trace",
      "TxPool",
      "Web3",
      "Personal",
      "Proof",
      "Net",
      "Parity",
      "Health"
    ],
    "Host": "127.0.0.1",
    "Port": 8545,
    "WebSocketsPort": 8546
  },
  "KeyStore": {
    "KeyStoreDirectory": "/nethermind/keystore",
    "PasswordFiles": ["/nethermind/bootnode.pwd"]
  },
  "Mining": {
    "Enabled": false
  },
  "Aura": {
    "AllowAuRaPrivateChains": true
  },
  "Pruning": {
    "Mode": "None"
  },
  "Sync": {
    "UseGethLimitsInFastBlocks": false,
    "FastSync": true
  }
}
